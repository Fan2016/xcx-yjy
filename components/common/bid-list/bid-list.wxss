/* 通用 */
.bid-list{
  /* background:#efefef; */
  /* height: 100%; */
  padding-top:20rpx;
  position: relative;
}
.trade-count-list{
  display: flex;
  margin-bottom: 20rpx;
  padding: 0 20rpx;
}
.trade-count-list .item{
  background: #f5f5f5;
  flex: 1;
  margin: 0 5rpx;
  font-size: 22rpx;
  font-weight: bold;
  text-align: center;
  height: 48rpx;
  line-height: 48rpx;
}
.trade-count-list .item.cur{
  background: #43abed;
  color:#fff;
}
image.onSubscription{
  width:200rpx;
height:200rpx;
display:block;
margin:0 auto 10rpx;
}
.bid-list-box,.inform-list-box{
  margin:0 20rpx 20rpx 20rpx;
  padding: 40rpx 20rpx 20rpx 20rpx; 
  /* border-bottom: 1px solid #f4f4f4; */
  border: 1rpx solid #eaeaea;
  border-radius: 10rpx;
  background: #fff;
  box-shadow: #efefef 0 5rpx 10rpx;
  position: relative;
  overflow: hidden;
}
.arrowView {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  height: 80rpx;
  line-height: 80rpx;
  position: relative;
  background-color: #f5f5f5;
  color:#999999;
  font-size: 30rpx;
}
.m0_20_20{
  margin: 0 20rpx 20rpx;
}

 .arrowView .info {
  
  margin-left: 20rpx
}
.arrowView .info text{
  color:#47A3FE;
  margin-right: 5rpx
}
.arrow_right {
  display: inline-block;
  border-top: 2px solid;
  border-right: 2px solid;
  width: 12px;
  height: 12px;
  border-color: #e4e4e4;
  transform: rotate(45deg);
  position: absolute;
  right: 25rpx;
  top: 30rpx;
}

.arrow_bottom {
  display: inline-block;
  border-top: 2px solid;
  border-right: 2px solid;
  width: 12px;
  height: 12px;
  border-color: #e4e4e4;
  transform: rotate(135deg);
  position: absolute;
  right: 25rpx;
  top: 20rpx;
}
.blue {
  background: #1983d1;
  width:7rpx;
  height:80rpx;
}

.orange {
  background: #f16840;
   width:7rpx;
  height:80rpx;
}
