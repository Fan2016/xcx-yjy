.order{
  position: absolute;
  width: 0;
    height: 0;
    border-top: 60rpx solid #7ec3f6;
    border-right: 60rpx solid transparent;
  left: 0;
  top:0;
}
.order text{
  position: absolute;
  color:#fff;
   left:0;
  top:-55rpx;
  font-size: 20rpx;
  width:32rpx;
height:32rpx;
text-align:center;

}
.new{
  position: absolute;
  width: 0;
    height: 0;
    border-top: 60rpx solid red;
    border-left: 60rpx solid transparent;
  /* background: yellowgreen; */
  right: 0;
  top:0;
  /* border-top-right-radius:5rpx  */
}
.new text{
  position: absolute;
  color:#fff;
   right: 0;
  top:-55rpx;
  font-size: 20rpx;
  transform:rotate(45deg);
}
.bid-tit{
  display: flex;
  border-bottom:1rpx solid #f5f5f5;
padding-bottom:10rpx;

}
.bid-tit .state{
  border-radius: 48rpx;
  width: 48rpx;
  height: 48rpx;
  line-height: 48rpx;
  text-align: center;
  color: #fff;
  font-size: 22rpx;
  margin-right: 10rpx;
  font-weight: bold;
}
