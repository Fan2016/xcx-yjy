/* components/common/yjy-money/yjy-money.wxss */

.line-bar {
  color: #666;
  font-size: 28rpx;
}

.line-bar .li {
  display: flex;
  align-items: center;
  height: 100rpx;
  text-align: left;
  padding-left: 30rpx;
  border-bottom: 1rpx solid #e1e1e1;
}

.line-bar .cur{
  color:#1983d1;
}


.custom button {
  background: #5a85ff;
  color: #fff;
}

.custom input {
  border: 1px solid #e1e1e1;
  width: 200rpx;
  height: 40rpx;
  text-align: center;
  margin: 0 10rpx;
}
