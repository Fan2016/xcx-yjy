/* pages/user/message/message.wxss */
page{
  background: #efefef;
}
.tabs{
z-index: 10;
width: 100%;
position: fixed;
top: 0;
  display: flex;
  background: #fff;
}
.tabs view{
  flex: 1;
  position: relative;
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
  color: #666666FF;
}
.tabs .cur{
  color: #4188F2FF
}
.tabs .cur:after {
position: absolute;
content: "";
width: 100rpx;
height: 6rpx;
box-sizing: border-box;
background: #49a2ff;
border-radius: 50rpx;
bottom: 0;
right: 35%;
}
.container-body{
  margin-top:80rpx
}
