.bid-tit .title{
overflow:hidden;
text-overflow:ellipsis;
white-space:nowrap;
flex:1;
height:48rpx;
line-height:48rpx;
font-size:32rpx;
color:#333;
font-weight:bold;

  
  /* font-weight: bold; */
}
.bid-tit image{
  width: 35rpx;
  height: 35rpx;
  padding-top: 5rpx;
}
.bid-tag{
  margin: 10rpx 0 10rpx 0;
  padding-left:50rpx;

}
.bid-tag text{
  display: inline-block;
  /* border: 1px solid #acdeff; */
  padding: 0 20rpx;
  margin-right: 20rpx;
  /* border-radius: 36rpx; */
  /* color: #62a8ff; */
  font-size: 22rpx;
  /* background: #edf8ff; */
}
.bid-tag .color-a{
  color:#95c3fd;
  background:#f2faff; 
}
.bid-tag .color-b{
  color:#f8c05e;
  background:#fef9f0; 
}
.bid-tag .color-c{
  color:#999999;
  background:#f6f6f6; 
}
.bid-tag .color-e{
  color:#b4dbad;
  background:#f9fef0;
}
.bid-info{
  font-size: 24rpx;
  color:#959595;
  display: flex;
  padding-left:50rpx;

}
.bid-info .color-d{
  color:#e89c4c;
}
.bid-info view{
  flex: 1
}
.bid-info view.right{
  flex: auto;
  width: 130rpx;
}

.bid-info icon{
  margin-right: 10rpx
}
.message{
  text-align: center;
  padding: 20rpx 0;
  color:#959595;
  font-size: 30rpx;
}
.message .load{
  width: 20rpx;
  height: 20rpx;
  margin-right:10rpx;
}
.onSearch{
  width:200rpx;
height:200rpx;
display:block;
margin:50rpx auto 10rpx;
}