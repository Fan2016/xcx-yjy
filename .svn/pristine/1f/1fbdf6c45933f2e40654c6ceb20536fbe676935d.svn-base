var app = getApp(),
  commonJs = require('../../../utils/common'),
  ajax = commonJs.ajax
Page({
  /**
   * 页面的初始数据
   */
  data: {
    indexMenu: [{
        IMG: '../../../images/info.png',
        NAME: '信息完善',
        isNew: false,
      },
      {
        IMG: '../../../images/myCollect.png',
        NAME: '我的关注',
        isNew: true,
      },
      {
        IMG: '../../../images/account.png',
        NAME: '账号管理',
        isNew: false,
      },
      {
        IMG: '../../../images/join.png',
        NAME: '我的参与',
        isNew: true,
      },
      {
        IMG: '../../../images/help.png',
        NAME: '帮助中心',
        isNew: false,
      },
      {
        IMG: '../../../images/advice.png',
        NAME: '意见反馈',
        isNew: false,
      }
    ],
    payMenu: [{
        IMG: '../../../images/pay.png',
        NAME: '账户充值'
      },
      {
        IMG: '../../../images/jifen.png',
        NAME: '积分使用'
      }
    ],
    userInfo: {
      nickName: "游客",
      avatarUrl: "../../../images/userIcon.png",
    },
    isLogin: false, //是否登陆
    impower: false, //是否微信授权
    userName: '游客'
  },
  onGotUserInfo: function (e) {
    //授权允许
    if (e.detail.errMsg =="getUserInfo:ok")
    {
      this.setData({
        userInfo: e.detail.userInfo,
        impower:true,
      })
      app.userInfo = e.detail.userInfo;
      app.impower = true;
    }
    else {//授权拒绝
      this.setData({
        impower: false,
      })
      app.userInfo = {};
      app.impower = false;
      console.log("拒绝授权")
    }
  },
  getIndexData() {
    let that = this
    ajax({ //获取行业类型
      url: 'http://192.168.118.154:9001/api/GetDataHandler.ashx',
      data: {
        method: 'Web.XCX_GetGroupByType'
      }
    }).then((res) => {
      var data = res.data.data
      that.setData({
        indexmenu: data
      })
    })
  },
  unbundle() {
    var that=this,username = this.data.userName;
    wx.showModal({
      title: '提示',
      content: '确认解绑用户',
      success(res) {
        if (res.confirm) {
          wx.login({ //用户是否注册绑定手机号
            success(res) {
              ajax({
                url:'http://192.168.118.154:6006/Login/LogOut',
                data: {
                  code: res.code,
                  username: username
                }
              }).then((res) => {
                var data = res.data;
                if (res.data.status == 200) {
                  app.token_type = ''
                  app.access_token = ''
                  app.userName = ''
                  that.setData({
                    isLogin:false,
                    userName:'游客'
                  })
                  console.log('解绑成功')
                }else{
                  console.log(data.msg)
                }
              })
            }
          })
        } else if (res.cancel) {

        }
      }
    })
  },
  /**
   * 生命周期函数--监听页面加载
   */
  onLoad: function(options) {
    var userInfo = this.data.userInfo;
    var impower = this.data.impower;
    var isLogin = this.data.isLogin;
    var userName = app.userName || this.data.userName
    wx.setNavigationBarTitle({
      title: '我的',
      success: function(res) {
        // success
      }
    })
    //是否授权
    if (app.impower) {
      userInfo = app.userInfo;
    }
    if (app.access_token && app.token_type) {
      isLogin = true;
    }
    this.setData({
      impower: app.impower,
      userInfo: userInfo,
      isLogin: isLogin,
      userName: userName,
    })

  },

  /**
   * 生命周期函数--监听页面初次渲染完成
   */
  onReady: function() {
 
  },

  /**
   * 生命周期函数--监听页面显示
   */
  onShow: function() {
  },

  /**
   * 生命周期函数--监听页面隐藏
   */
  onHide: function() {

  },

  /**
   * 生命周期函数--监听页面卸载
   */
  onUnload: function() {

  },

  /**
   * 页面相关事件处理函数--监听用户下拉动作
   */
  onPullDownRefresh: function() {
    console.log('Down')
  },

  /**
   * 页面上拉触底事件的处理函数
   */
  onReachBottom: function() {

  },
  /**
   * 用户点击右上角分享
   */
  onShareAppMessage: function() {

  }
})
